        </activity>

        <activity
            android:name=".InsertContactActivity"
            android:label="@string/title_activity_insert_contact"></activity>
        <activity
            android:name=".SearchContactActivity"
            android:label="@string/title_activity_search_contact"></activity>
        <activity
            android:name=".AllContactsActivity"
